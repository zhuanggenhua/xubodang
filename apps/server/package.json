    "eslint": "^8.26.0",
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "@types/fs-extra": "^11.0.1",
    "chalk": "^4.1.2",
    "dayjs": "^1.11.9",
    "fs-extra": "^10.1.0",
    "@grpc/grpc-js": "^1.8.14",
    "@grpc/proto-loader": "^0.7.6",
    "grpc-tools": "~1.11.3",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "google-protobuf": "^3.21.2",
